<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE entry [
	<!ENTITY ipath "icons/">
	<!ENTITY etool "&ipath;etool16">
	<!ENTITY dtool "&ipath;dtool16">
	<!ENTITY tango "&ipath;tango/16x16">
]>
<entry entryPoint="nomad"
	xmlns="http://nmedit.sf.net/MenuLayout"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <entry entryPoint="menu" >
	<entry entryPoint="file" >
		<entry entryPoint="new">
			<entry entryPoint="global" flat="true">
				<entry entryPoint="location">
					<icon type="enabledIcon" src="&tango;/actions/folder-new.png" />
				</entry>
				<entry entryPoint="synth">
					<icon type="enabledIcon" src="&etool;/new_wiz.gif" />
					<icon type="disabledIcon" src="&dtool;/new_wiz.gif" />
				</entry>
			</entry>
		</entry>
		<entry entryPoint="open">
		  <icon type="enabledIcon" src="&tango;/actions/document-open.png" />
		</entry>
		<entry entryPoint="close" flat="true">
			<entry entryPoint="close">
		    </entry>
			<entry entryPoint="closeall">
		    </entry>
		</entry>
		<entry entryPoint="save" flat="true">
			<entry entryPoint="save">
				<icon type="enabledIcon" src="&tango;/actions/document-save.png" />
				<icon type="disabledIcon" src="&tango;/actions/document-save.png" />
			</entry>
			<entry entryPoint="saveas">
				<icon type="enabledIcon" src="&tango;/actions/document-save-as.png" />
				<icon type="disabledIcon" src="&tango;/actions/document-save-as.png" />
			</entry>
			<entry entryPoint="saveall">
				<icon type="enabledIcon" src="&tango;/actions/document-save-as.png" />
				<icon type="disabledIcon" src="&tango;/actions/document-save-as.png" />
			</entry>
			<entry entryPoint="saveinsynth" />
		</entry>
		<entry entryPoint="ie" flat="true">
			<entry entryPoint="import">
				<icon type="enabledIcon" src="&etool;/import_wiz.gif" />
				<icon type="disabledIcon" src="&dtool;/import_wiz.gif" />
				<accelerator desc="control I" descmac="meta I"/>
			</entry>
			<entry entryPoint="export">
				<icon type="enabledIcon" src="&etool;/export_wiz.gif" />
				<icon type="disabledIcon" src="&dtool;/export_wiz.gif" />
				<accelerator desc="control E" descmac="meta E"/>
			</entry>
		</entry>
		<entry entryPoint="properties" >
			<icon type="enabledIcon" src="&tango;/actions/document-properties.png" />
			<icon type="disabledIcon" src="&tango;/actions/document-properties.png" />
     	</entry>
		<entry entryPoint="exit" />
	</entry>
	<entry entryPoint="edit">
		<entry entryPoint="history" flat="true">
			<entry entryPoint="undo">
				<icon type="enabledIcon" src="&tango;/actions/edit-undo.png"/>
				<icon type="disabledIcon" src="&tango;/actions/edit-undo.png"/>
			</entry>
			<entry entryPoint="redo">
				<icon type="enabledIcon" src="&tango;/actions/edit-redo.png" />
				<icon type="disabledIcon" src="&tango;/actions/edit-redo.png" />
			</entry>
		</entry>
		<entry entryPoint="edit" flat="true">
			<entry entryPoint="cut">
				<icon type="enabledIcon" src="&tango;/actions/edit-cut.png" />
				<icon type="disabledIcon" src="&tango;/actions/edit-cut.png" />
			</entry>
			<entry entryPoint="copy">
				<icon type="enabledIcon" src="&tango;/actions/edit-copy.png" />
				<icon type="disabledIcon" src="&tango;/actions/edit-copy.png" />
			</entry>
			<entry entryPoint="paste">
				<icon type="enabledIcon" src="&tango;/actions/edit-paste.png" />
				<icon type="disabledIcon" src="&tango;/actions/edit-paste.png" />
			</entry>
		</entry>
		<entry entryPoint="app" flat="true">
			<entry entryPoint="preferences">
				<icon type="enabledIcon" src="&tango;/actions/document-properties.png" />
				<icon type="disabledIcon" src="&tango;/actions/document-properties.png" />
			</entry>
		</entry>
	</entry>
	<!--
	<entry entryPoint="synth">
		<entry entryPoint="new" />
		<entry entryPoint="setup" />
	</entry>
	-->
	<entry entryPoint="patch">
		<entry entryPoint="notes" />
		<entry entryPoint="settings" />
		<entry entryPoint="randomize">
		</entry>
		<entry entryPoint="knobs"/>
	</entry>
	<!--
	<entry entryPoint="window">
		<entry entryPoint="language">
			<entry entryPoint="de_DE" />
			<entry entryPoint="en" />
			<entry entryPoint="fr_FR" />
		</entry>
	</entry> -->
	<entry entryPoint="help">
		<entry entryPoint="plugins">
			<icon type="enabledIcon" src="&tango;/apps/help-browser.png" />
		</entry>
		<entry entryPoint="license">
			<icon type="enabledIcon" src="&tango;/apps/help-browser.png" />
			<icon type="disabledIcon" src="&tango;/apps/help-browser.png" />
		</entry>
		<entry entryPoint="about">
			<icon type="enabledIcon" src="&tango;/apps/help-browser.png" />
			<icon type="disabledIcon" src="&tango;/apps/help-browser.png" />
		</entry>
		<entry entryPoint="debug">
			<entry entryPoint="gc" />
		</entry>
	</entry>
  </entry> <!-- end of menu -->

  <entry entryPoint="explorer">
    <entry entryPoint="synth">
      <entry entryPoint="control" flat="true">
        <entry entryPoint="connect" />
        <entry entryPoint="disconnect" />
      </entry>
      <entry entryPoint="settings" />
      <entry entryPoint="general" flat="true">
        <entry entryPoint="remove" />
      </entry>
    </entry>
  </entry>
  
  <entry entryPoint="fileexplorer">
    <entry entryPoint="item" flat="true">
      <entry entryPoint="open">
		  <icon type="enabledIcon" src="&tango;/actions/document-open.png" />
		</entry>
      <entry entryPoint="delete">
		  <icon type="enabledIcon" src="&tango;/actions/edit-delete.png" />
		</entry>
    </entry>
    <entry entryPoint="rename" />
    <entry entryPoint="entry" flat="true">
      <entry entryPoint="refresh">
		  <icon type="enabledIcon" src="&tango;/actions/view-refresh.png" />
		</entry>
      <entry entryPoint="remove"  >
		  <icon type="enabledIcon" src="&tango;/actions/list-remove.png" />
		</entry>
    </entry>
    <entry entryPoint="dir" flat="true">
      <entry entryPoint="newdir" >
		  <icon type="enabledIcon" src="&tango;/actions/folder-new.png" />
		</entry>
    </entry>
  </entry>
</entry>
